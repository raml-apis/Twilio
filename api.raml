          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                AreaCode: 
                  description: |
                    Find phone numbers in the specified area code. (US and Canada only).
                Contains: 
                  description: |
                    A pattern to match phone numbers on. Valid characters are "*" and
