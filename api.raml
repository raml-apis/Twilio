/Accounts{mediaTypeExtension}: 
  type: { collection: { type: account } }
  get: 
    description: |
      Retrieve a list of the Account resources belonging to the account used to make the
      API request. This list will include that Account as well.
    body: 
      application/x-www-form-urlencoded: 
        formParameters: 
          FriendlyName: 
            description: |
              Only return the Account resources with friendly names that exactly match this name.
          Status: 
            description: |
              Only return Account resources with the given status. Can be closed, suspended or active.
            enum: 
              - closed
              - suspended
              - active
    responses: 
      200: 
        body: 
          application/xml: 
            example: !include examples/accounts-xml-example.xml
          application/json: 
            example: !include examples/accounts-example.json
  post: 
    description: |
      Create a new Account instance resource as a subaccount of the one used to make the request. See
      Creating Subaccounts for more information.
    body: 
      application/x-www-form-urlencoded: 
        formParameters: 
          FriendlyName: 
            description: |
              A human readable description of the new subaccount, up to 64 characters. Defaults
              to "SubAccount Created at {YYYY-MM-DD HH:MM meridian}".
    responses: 
      201: 
        body: 
          application/xml: 
            example: !include examples/account-xml-example.xml
          application/json: 
            example: !include examples/account-example.json
/Accounts: 
  /{AccountSid}{mediaTypeExtension}: 
    type: { collectionMember: { type: account } }
    uriParameters: 
      AccountSid: 
        description: A 34 character string that uniquely identifies this account.
        minLength: 34
        maxLength: 34
    get: 
      description: Returns a representation of an account.
      responses: 
        200: 
          body: 
            application/xml: 
              example: !include examples/account-xml-example.xml
            application/json: 
              example: !include examples/account-example.json
    post: 
      description: Allows you to modify the properties of an account.
      body: 
        application/x-www-form-urlencoded: 
          formParameters: 
            FriendlyName: 
              description: Update the human-readable description of this account.
            Status: 
              description: |
                Alter the status of this account: use closed to irreversibly close this account,
                suspended to temporarily suspend it, or active to reactivate it.
              enum: 
                - closed
                - suspended
                - active
      responses: 
        200: 
          body: 
            application/xml: 
              example: !include examples/account-xml-example.xml
            application/json: 
              example: !include examples/account-example.json
    put: 
      description: Allows you to modify the properties of an account.
      body: 
        application/x-www-form-urlencoded: 
          formParameters: 
            FriendlyName: 
              description: Update the human-readable description of this account.
            Status: 
              description: |
                Alter the status of this account: use closed to irreversibly close this account,
                suspended to temporarily suspend it, or active to reactivate it.
              enum: 
                - closed
                - suspended
                - active
      responses: 
        200: 
          body: 
            application/xml: 
              example: !include examples/account-xml-example.xml
            application/json: 
              example: !include examples/account-example.json
  /{AccountSid}: 
    /SMS: 
      /ShortCodes{mediaTypeExtension}: 
        type: { collection: { type: shortCode } }
        get: 
          description: |
            Returns a list of ShortCode resource representations, each representing a
            short code within your account. The list includes paging information.
          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                ShortCode: 
                  description: |
                    Only show the ShortCode resources that match this pattern. You can
                    specify partial numbers and use "*" as a wildcard for any digit.
                FriendlyName: 
                  description: |
                    Only show the ShortCode resources with friendly names that exactly match
                    this name.
          responses: 
            200: 
              body: 
                application/xml: 
                  example: !include examples/shortCodes-xml-example.xml
                application/json: 
                  example: !include examples/shortCodes-example.json
      /ShortCodes/{ShortCodeSid}{mediaTypeExtension}: 
        type: { collectionMember: { type: shortCode } }
        get: 
          description: Get a single message.
          responses: 
            200: 
              body: 
                application/xml: 
                  example: !include examples/shortCode-xml-example.xml
                application/json: 
                  example: !include examples/shortCode-example.json
        post: 
          description: |
            Tries to update the shortcode's properties, and returns the updated
            resource representation if successful.
          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                FriendlyName: 
                  description: |
                    A human readable description of the short code, with maximum
                    length 64 characters.
                  maxLength: 64
                ApiVersion: 
                  description: |
                    SMSs to this short code will start a new TwiML session with this
                    API version. Either 2010-04-01 or 2008-08-01.
                  enum: [ 2010-04-01 , 2008-08-01 ]
                SmsUrl: 
                  description: |
                    The URL that Twilio should request when somebody sends an SMS to
                    the short code.
                SmsMethod: 
                  description: |
                    The HTTP method that should be used to request the SmsUrl. Either
                    GET or POST.
                  enum: [ GET , POST ]
                SmsFallbackUrl: 
                  description: |
                    A URL that Twilio will request if an error occurs requesting or
                    executing the TwiML at the SmsUrl.
                SmsFallbackMethod: 
                  description: |
                    The HTTP method that should be used to request the SmsUrl. Either
                    GET or POST.
                  enum: [ GET , POST ]
          responses: 
            200: 
              body: 
                application/xml: 
                  example: !include examples/shortCode-xml-example.xml
                application/json: 
                  example: !include examples/shortCode-example.json
    /Notifications{mediaTypeExtension}: 
      type: { collection: { type: notification } }
      get: 
        description: |
          Returns a list of notifications generated for an account. The list includes
          paging information.
        body: 
          application/x-www-form-urlencoded: 
            formParameters: 
              Log: 
                description: |
                  Only show notifications for this log, using the integer log values
                  shown above.
              MessageDate: 
                description: |
                  Only show notifications for this date. Should be formatted as YYYY-MM-DD.
                  You can also specify inequality, such as MessageDate<=YYYY-MM-DD for
                  messages logged at or before midnight on a date, and MessageDate>=YYYY-MM-DD
                  for messages logged at or after midnight on a date.
        responses: 
          200: 
            body: 
              application/xml: 
                example: !include examples/notifications-xml-example.xml
              application/json: 
                example: !include examples/notifications-example.json
    /Notifications/{NotificationSid}{mediaTypeExtension}: 
      type: { collectionMember: { type: notification } }
      get: 
        description: Get a notification entry.
        responses: 
          200: 
            body: 
              application/xml: 
                example: !include examples/notification-xml-example.xml
              application/json: 
                example: !include examples/notification-example.json
      delete: 
        description: |
          Deletes the notification identified by {NotificationSid} from an account's log.
        responses: 
          204: 
            description: Notification removed.
    /Applications{mediaTypeExtension}: 
      type: { collection: { type: application } }
      get: 
        description: |
          Returns a list of Application resource representations, each representing
          an application within your account. The list includes paging information.
        body: 
          application/x-www-form-urlencoded: 
            formParameters: 
              FriendlyName: 
                description: |
                  Only return the Application resources with friendly names that exactly
                  match this name.
        responses: 
          200: 
            body: 
              application/xml: 
                example: !include examples/applications-xml-example.xml
              application/json: 
                example: !include examples/applications-example.json
      post: 
        is: [ customizable ]
        description: Creates a new application within your account.
        body: 
          application/x-www-form-urlencoded: 
            formParameters: 
              FriendlyName: 
                description: |
                  A human readable description of the application, with maximum
                  length 64 characters.
                required: true
                maxLength: 64
        responses: 
          201: 
            body: 
              application/xml: 
                example: !include examples/application-xml-example.xml
              application/json: 
                example: !include examples/application-example.json
    /Applications/{ApplicationSid}{mediaTypeExtension}: 
      type: { collectionMember: { type: application } }
      get: 
        description: Get application instance resource.
        responses: 
          200: 
            body: 
              application/xml: 
                example: !include examples/application-xml-example.xml
              application/json: 
                example: !include examples/application-example.json
      post: 
        is: [ customizable ]
        description: |
          Tries to update the application's properties, and returns the updated
          resource representation if successful. The returned response is identical
          to that returned above when making a GET request.
        body: 
          application/x-www-form-urlencoded: 
            formParameters: 
              FriendlyName: 
                description: |
                  A human readable description of the application, with maximum
                  length 64 characters.
                maxLength: 64
        responses: 
          200: 
            body: 
              application/xml: 
                example: !include examples/application-xml-example.xml
              application/json: 
                example: !include examples/application-example.json
      delete: 
        description: Delete this application.
        responses: 
          204: 
            description: Application deleted.
    /IncomingPhoneNumbers{mediaTypeExtension}: 
      type: { collection: { type: incomingCall } }
      get: 
        description: |
          Returns a list of IncomingPhoneNumber resource representations, each
          representing a phone number given to your account. The list includes paging
          information.
        body: 
          application/x-www-form-urlencoded: 
            formParameters: 
              PhoneNumber: 
                description: |
                  Only show the incoming phone number resources that match this pattern.
                  You can specify partial numbers and use "*" as a wildcard for any digit.
              FriendlyName: 
                description: |
                  Only show the incoming phone number resources with friendly names tha
                  exactly match this name.
        responses: 
          200: 
            body: 
              application/xml: 
                example: !include examples/incomingCalls-xml-example.xml
              application/json: 
                example: !include examples/incomingCalls-example.json
      post: 
        is: [ customizable ]
        description: |
          Purchases a new phone number for your account. If a phone number is found
          for your request, Twilio will add it to your account and bill you for the
          first month's cost of the phone number.
          To find an available phone number to POST, use the subresources of the
          AvailablePhoneNumbers list resource.
        body: 
          application/x-www-form-urlencoded: 
            formParameters: 
              PhoneNumber: 
                description: |
                  The phone number you want to purchase. The number should be formatted
                  starting with a '+' followed by the country code and the number in
                  E.164 format e.g., '+15105555555'. You must include either this or
                  an AreaCode parameter to have your POST succeed.
                required: true
              AreaCode: 
                description: |
                  The desired area code for your new incoming phone number. Any three
                  digit, US or Canada area code is valid. Twilio will provision a random
                  phone number within this area code for you. You must include either
                  this or a PhoneNumber parameter to have your POST succeed. (US and
                  Canada only)
                required: true
              FriendlyName: 
                description: |
                  A human readable description of the new incoming phone number. Maximum
                  64 characters. Defaults to a formatted version of the number.
                maxLength: 64
              VoiceApplicationSid: 
                description: |
                  The 34 character sid of the application Twilio should use to handle phone calls to the new number.
                  If a VoiceApplicationSid is present, Twilio will ignore all of the voice urls above and use those set on the application.
        responses: 
          400: 
            description: If Twilio cannot find a phone number to match your request, you will receive an HTTP 400 with Twilio error code 21452.
          201: 
            body: 
              application/xml: 
                example: !include examples/incomingCall-xml-example.xml
              application/json: 
                example: !include examples/incomingCall-example.json
    /IncomingPhoneNumbers: 
      /{IncomingPhoneNumberSid}{mediaTypeExtension}: 
        type: { collectionMember: { type: incomingCall } }
        get: 
          description: "Get info about incoming call's phone number."
          responses: 
            200: 
              body: 
                application/xml: 
                  example: !include examples/incomingCall-xml-example.xml
                application/json: 
                  example: !include examples/incomingCall-example.json
        post: 
          is: [ customizable ]
          description: |
            Tries to update the incoming phone number's properties, and returns the
            updated resource representation if successful. The returned response is
            identical to that returned above when making a GET request.
          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                FriendlyName: 
                  description: |
                    A human readable description of the new incoming phone number
                    resource, with maximum length 64 characters.
                VoiceApplicationSid: 
                  description: |
                    The 34 character sid of the application Twilio should use to handle phone calls to the new number.
                    If a VoiceApplicationSid is present, Twilio will ignore all of the voice urls above and use those set on the application.
          responses: 
            200: 
              body: 
                application/xml: 
                  example: !include examples/incomingCall-xml-example.xml
                application/json: 
                  example: !include examples/incomingCall-example.json
        delete: 
          description: |
            Release this phone number from your account. Twilio will no longer answer
            calls to this number, and you will stop being billed the monthly phone
            number fee. The phone number will eventually be recycled and potentially
            given to another customer, so use with care. If you make a mistake, contac
            us. We may be able to give you the number back.
          responses: 
            204: 
              description: Phone number removed.
        put: 
          is: [ customizable ]
          description: |
            Tries to update the incoming phone number's properties, and returns the
            updated resource representation if successful. The returned response is
            identical to that returned above when making a GET request.
          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                FriendlyName: 
                  description: |
                    A human readable description of the new incoming phone number
                    resource, with maximum length 64 characters.
                VoiceApplicationSid: 
                  description: |
                    The 34 character sid of the application Twilio should use to handle phone calls to the new number.
                    If a VoiceApplicationSid is present, Twilio will ignore all of the voice urls above and use those set on the application.
          responses: 
            200: 
              body: 
                application/xml: 
                  example: !include examples/incomingCall-xml-example.xml
                application/json: 
                  example: !include examples/incomingCall-example.json
      /Local{mediaTypeExtension}: 
        get: 
          description: Returns a list of local <IncomingPhoneNumber> elements, each representing a local (not toll-free) phone number given to your account, under an <IncomingPhoneNumbers> list element that includes paging information. Works exactly the same as the IncomingPhoneNumber resource, but filters out toll-free numbers.
          responses: 
            200: 
        post: 
          is: [ customizable ]
          description: "Adds a new phone number to your account. If a phone number is found for your request, Twilio will add it to your account and bill you for the first month's cost of the phone number. "
          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                PhoneNumber: 
                  required: true
                  example: +15107522575
          responses: 
            400: 
              description: "If Twilio can't find a phone number to match your request, you will receive an HTTP 400 with Twilio error code 21452."
            201: 
      /TollFree{mediaTypeExtension}: 
        get: 
          description: "Returns a list of local <IncomingPhoneNumber> elements, each representing a toll-free phone number given to your account, under an <IncomingPhoneNumbers> list element that includes paging information. Works exactly the same as the IncomingPhoneNumber resource, but filters out all numbers that aren't toll-free."
          responses: 
            200: 
        post: 
          is: [ customizable ]
          description: "Adds a new phone number to your account. If a phone number is found for your request, Twilio will add it to your account and bill you for the first month's cost of the phone number. "
          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                PhoneNumber: 
                  required: true
                  example: +15107522575
          responses: 
            400: 
              description: "If Twilio can't find a phone number to match your request, you will receive an HTTP 400 with Twilio error code 21452."
            201: 
      /Mobile{mediaTypeExtension}: 
        get: 
          description: Returns a list of local <IncomingPhoneNumber> elements, each representing a mobile phone number given to your account, under an <IncomingPhoneNumbers> list element that includes paging information. Works exactly the same as the IncomingPhoneNumber resource, but filters out local and toll free numbers.
          responses: 
            200: 
        post: 
          is: [ customizable ]
          description: "Adds a new phone number to your account. If a phone number is found for your request, Twilio will add it to your account and bill you for the first month's cost of the phone number."
          body: 
            application/x-www-form-urlencoded: 
              formParameters: 
                PhoneNumber: 
                  required: true
                  example: +15107522575
          responses: 
            400: 
              description: "If Twilio can't find a phone number to match your request, you will receive an HTTP 400 with Twilio error code 21452."
            201: 
    /Calls{mediaTypeExtension}: 
      type: { collection: { type: call } }
      post: 
        description: To make a call, make an HTTP POST request. Initiate a new phone call.
        body: 
          application/x-www-form-urlencoded: 
            formParameters: 
              From: 
                description: |
                  The phone number or client identifier to use as the caller id. If
                  using a phone number, it must be a Twilio number or a Verified
                  outgoing caller id for your account.
                  Phone numbers should be formatted with a '+' and country code e.g.,
                  +16175551212 (E.164 format). Twilio will also accept unformatted US
                  numbers e.g., (415) 555-1212 or 415-555-1212.
                required: true
              To: 
                description: |
                  SIP addresses must be formatted as name@example.com. For example,
                  to dial Alice's SIP address at Example Company, the To parameter
                  should be alice@example.com.
                  Client identifiers must begin with the client: URI scheme. For example,
                  to call a client named 'jenny', the To parameter should be client:jenny.
                required: true
              Url: 
                description: |
                  The fully qualified URL that should be consulted when the call connects.
                  Just like when you set a URL on a phone number for handling inbound calls.
              ApplicationSid: 
                description: |
                  The 34 character sid of the application Twilio should use to handle
                  this phone call. If this parameter is present, Twilio will ignore all
                  of the voice URLs passed and use the URLs set on the application.
                minLength: 34
                maxLength: 34
              Method: 
                description: |
                  The HTTP method Twilio should use when making its request to the above
                  Url parameter's value. Defaults to POST. If an ApplicationSid parameter
                  is present, this parameter is ignored.
                enum: [ GET , POST ]
                default: POST
              FallbackUrl: 
                description: |
                  A URL that Twilio will request if an error occurs requesting or
                  executing the TwiML at Url. If an ApplicationSid parameter is present,
                  this parameter is ignored.
              FallbackMethod: 
                description: |
                  The HTTP method that Twilio should use to request the FallbackUrl.
                  Must be either GET or POST. Defaults to POST. If an ApplicationSid
                  parameter is present, this parameter is ignored.
                enum: [ GET , POST ]
                default: POST
              StatusCallback: 
                description: |
                  A URL that Twilio will request when the call ends to notify your
                  app. If an ApplicationSid parameter is present, this parameter is
                  ignored.
              StatusCallbackMethod: 
                description: |
                  The HTTP method Twilio should use when requesting the above URL.
                  Defaults to POST. If an ApplicationSid parameter is present, this
                  parameter is ignored.
                enum: [ GET , POST ]
                default: POST
              SendDigits: 
                description: |
                  A string of keys to dial after connecting to the number. Valid digits
                  in the string include: any digit (0-9), '#', "*" and 'w' (to inser
                  a half second pause). For example, if you connected to a company
                  phone number, and wanted to pause for one second, dial extension
                  1234 and then the pound key, use SendDigits=ww1234#. Remember to
                  URL-encode this string, since the '#' character has special meaning
                  in a URL.
              IfMachine: 
                description: |
                  Tell Twilio to try and determine if a machine (like voicemail) or
                  a human has answered the call. Possible values are Continue and Hangup.
                enum: [ Continue , Hangup ]
              Timeout: 
                description: |
                  The integer number of seconds that Twilio should allow the phone to
                  ring before assuming there is no answer. Default is 60 seconds, the
                  maximum is 999 seconds. Note, you could set this to a low value, such
                  as 15, to hangup before reaching an answering machine or voicemail.
                type: integer
              Record: 
                description: |
                  Set this parameter to 'true' to record the entirety of a phone call.
                  The RecordingUrl will be sent to the StatusCallback URL.
                type: boolean
                default: false
              SipAuthUsername: 
              SipAuthPassword: 
        responses: 
          201: 
            body: 
              application/xml: 
                example: !include examples/call-xml-example.xml
              application/json: 
                example: !include examples/call-example.json
      get: 
